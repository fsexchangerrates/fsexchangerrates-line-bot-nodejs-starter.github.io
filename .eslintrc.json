{
    "extends": "standard",
    "rules": {
        "semi": [1, "always"],
        "comma-dangle": [0, "standard"],
        "space-before-function-paren": [0, "always"],
        "jsx-quotes": ["off", "prefer-double"],
        "space-before-blocks": "off",
        "indent":"off",
        "eol-last":"off"
      }
}